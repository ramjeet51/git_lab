#animate{
	height:80%;
	width:100%;
	margin-top:100px;
	animation:ani 5s linear infinite;
	position:relative;
}
@keyframes ani{
	0%{
		top:0;
	}
	50%{
		top:-50px;
	}
	100%{
		top:0px;
	}
}
